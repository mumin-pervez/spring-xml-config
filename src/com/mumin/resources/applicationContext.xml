<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

  <!-- bean definitions here -->
  <bean class="com.mumin.beans.Doctor" id="doctor1">
    <property name="id" value="101"/>
    <property name="name" value="Dr. Manirul Islam Khan"/>
    <property name="department" value="Medicine"/>
  </bean>

  <bean class="com.mumin.beans.Doctor" id="doctor2">
    <property name="id" value="108"/>
    <property name="name" value="Dr. Alim Al Razin"/>
    <property name="department" value="Orthopaedic"/>
  </bean>

</beans>